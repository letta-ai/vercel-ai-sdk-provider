{
  "name": "@letta-ai/vercel-ai-sdk-provider",
  "version": "0.1.47",
  "license": "MIT",
  "sideEffects": false,
  "main": "test-apps/letta-ai-sdk-example/node_modules/@letta-ai/vercel-ai-sdk-provider/dist/index.js",
  "module": "test-apps/letta-ai-sdk-example/node_modules/@letta-ai/vercel-ai-sdk-provider/dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf test-apps/letta-ai-sdk-example/node_modules/@letta-ai/vercel-ai-sdk-provider/dist",
    "lint": "eslint \"./**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "prettier-check": "prettier --check \"./**/*.ts*\"",
    "test": "npm run test:node && npm run test:e2e",
    "test:e2e": "vitest --config vitest.e2e.config.js --run",
    "test:e2e:local": "vitest --config vitest.e2e.local.config.js --run",
    "test:node": "vitest --config vitest.node.config.js --run",
    "test:node:watch": "vitest --config vitest.node.config.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@ai-sdk/provider": "1.1.0",
    "@ai-sdk/provider-utils": "2.2.4",
    "@ai-sdk/ui-utils": "^1.2.5",
    "@letta-ai/letta-client": "^0.1.176"
  },
  "devDependencies": {
    "@types/node": "20.17.24",
    "ai": "^4.3.19",
    "dotenv": "^17.2.0",
    "tsup": "^8",
    "typescript": "5.6.3",
    "vitest": "^3.1.1",
    "zod": "3.23.8"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://sdk.vercel.ai/docs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vercel/ai.git"
  },
  "bugs": {
    "url": "https://github.com/vercel/ai/issues"
  },
  "keywords": [
    "ai"
  ]
}
